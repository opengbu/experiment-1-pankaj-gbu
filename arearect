#include <iostream>
using namespace std;
int main()
{
	float	length,breadth, areaofrectangle;

	cout<< "give the value of length in m " ;
	cin>> length ;
	
	cout<< "give the value of breadth in m " ;
	cin>> breadth;
	
	areaofrectangle= (length*breadth);		
	cout<<"area of rectangle in (metresq) is " <<areaofrectangle ;
	return 0;
	



}
